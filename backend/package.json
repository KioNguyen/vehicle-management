{
  "name": "typeorm-sample",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "scripts": {
    "build": "tsc --build",
    "typeorm": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli -d src/database/datasource",
    "lint": "eslint --ext .ts src/ --fix",
    "start": "ts-node --transpile-only src/index.ts",
    "watch": "ts-node-dev --respawn --transpile-only src/index.ts",
    "migration:generate": "npm run typeorm -- migration:generate src/database/migrations/packing -p",
    "migration:run": "npm run typeorm -- migration:run"
  },
  "dependencies": {
    "@apollo/server": "^4.4.0",
    "@graphql-tools/schema": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "apollo-server-express": "^3.13.0",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.22.4",
    "graphql-tag": "^2.12.6",
    "graphql-type-json": "^0.3.2",
    "joiful": "^3.0.2",
    "lodash": "^4.17.21",
    "nodemon": "^3.0.2",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "2.0.0-beta.3",
    "typedi": "^0.10.0",
    "typeorm": "0.3.17",
    "typeorm-pagination": "^2.0.3"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^16.11.10",
    "dotenv": "^16.3.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.5.2"
  }
}